{
    "version": 2,
    "builds": [
      {
        "src": "api/api.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/api.js"
      }
    ],
    "functions": {
      "api/api.js": {
        "memory": 3008,
        "maxDuration": 30
      },
      "api/*.js": {
        "memory": 3008,
        "maxDuration": 30
      }
    }
  }